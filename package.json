{
   "name": "api",
   "description": "API servers with Koa and TypeScript",
   "version": "0.0.1",
   "license": "MIT",
   "engines": {
      "node": "14.x"
   },
   "scripts": {
      "start": "NODE_ENV=production node dist/index.js",
      "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
      "lint": "tslint --project ./tsconfig.json",
      "build": "tsc",
      "typeorm": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "migration:generate": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -n",
      "migration:run": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run && npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -c seed",
      "migration:revert": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -c seed && npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
      "entity:generate": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js entity:create -n ",
      "seed:generate": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -c seed -n ",
      "seed:run": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -c seed",
      "seed:revert": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -c seed"
   },
   "dependencies": {
      "@koa/cors": "^3.1.0",
      "@types/cors": "^2.8.8",
      "bcryptjs": "^2.4.3",
      "debug": "^3.1.0",
      "dotenv": "^8.2.0",
      "firebase": "^8.0.2",
      "firebase-admin": "^9.4.1",
      "jsonwebtoken": "^8.5.1",
      "koa": "^2.5.2",
      "koa-body": "^4.0.4",
      "koa-bodyparser": "^4.3.0",
      "koa-combine-routers": "^4.0.2",
      "koa-convert": "^1.2.0",
      "koa-helmet": "^4.0.0",
      "koa-http-request": "^2.2.1",
      "koa-jwt": "^3.5.0",
      "koa-morgan": "^1.0.1",
      "koa-router": "^7.4.0",
      "koa-static": "^5.0.0",
      "koa-yup-validator": "^1.0.7",
      "moment": "^2.29.1",
      "mysql": "^2.14.1",
      "pg": "^8.5.1",
      "ramda": "^0.25.0",
      "reflect-metadata": "^0.1.10",
      "request": "^2.88.0",
      "request-promise": "^4.2.2",
      "slugify": "^1.4.5",
      "typeorm": "0.2.26",
      "uuid": "^3.4.0",
      "winston": "^3.0.0",
      "yup": "^0.29.3"
   },
   "devDependencies": {
      "@babel/core": "^7.11.6",
      "@babel/preset-env": "^7.11.5",
      "@types/bcryptjs": "^2.4.2",
      "@types/jest": "^23.3.1",
      "@types/jsonwebtoken": "^8.5.0",
      "@types/koa": "^2.0.46",
      "@types/koa-bodyparser": "^4.3.0",
      "@types/koa-helmet": "^3.1.2",
      "@types/koa-morgan": "^1.0.4",
      "@types/koa-router": "^7.0.31",
      "@types/koa-static": "^4.0.1",
      "@types/koa__cors": "^3.0.2",
      "@types/morgan": "^1.7.35",
      "@types/node": "^8.0.29",
      "@types/ramda": "^0.25.36",
      "@types/request-promise": "^4.1.42",
      "@types/socket.io": "^2.1.11",
      "@types/supertest": "^2.0.5",
      "@types/uuid": "^3.4.3",
      "@types/yup": "^0.29.7",
      "@typescript-eslint/eslint-plugin": "2.31.0",
      "@typescript-eslint/parser": "2.31.0",
      "babel-core": "^6.26.3",
      "babel-jest": "^23.4.2",
      "babel-preset-env": "^1.7.0",
      "chai": "^4.1.2",
      "cross-env": "^5.2.0",
      "eslint": "^5.4.0",
      "eslint-config-prettier": "6.11.0",
      "eslint-plugin-prettier": "3.1.3",
      "eslint-plugin-security": "1.4.0",
      "forever": "^0.15.2",
      "jest": "^23.5.0",
      "mockery": "^2.1.0",
      "nodemon": "^1.18.3",
      "prettier": "2.0.5",
      "raml2html": "^7.1.0",
      "sinon": "^6.1.5",
      "supertest": "^3.1.0",
      "throng": "^4.0.0",
      "ts-jest": "^23.1.4",
      "ts-node": "^3.3.0",
      "ts-node-dev": "^1.0.0-pre.26",
      "tsconfig-paths": "^3.9.0",
      "typescript": "3.3.3333"
   }
}
